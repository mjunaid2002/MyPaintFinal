@model WebApplication1.ViewModels.SaleInvVM
@{Layout = null;
    var totalqty = Model.FinishTransferDetailList.Sum(x => x.qty);
   

    //var totalqty = Model.Sum(x => x.qty);
    //var totalsp = Model.Sum(x => x.sp);
    //var ttotal = Model.Sum(x => x.total);

}
<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <title>Transfer Finished Products </title>
    <!-- You can add additional meta tags, stylesheets, or scripts in the head section -->
</head>
<body>
    <div class="container text-center " style="margin-top:40px">
        <div style="display: flex; justify-content: space-between;">
            <div style="padding: 10px; width: 40%; text-align: left;font-size:15px;font-weight:bold">
                <div style="display: flex">
                    <div @*style="padding-right:60px; width: 30%;"*@>
                        <img style="max-width: 180px; height: auto;" src="~/Content/invoiceimg1.png" alt="Invoice Image" />
                    </div>
                    <div style=" padding: 10px; width:70%;text-align:left">

                        <div style="color:blue;font-size:18px;font-weight:bold">@ViewData["compname"]</div>
                        <div>@ViewData["email"]</div>
                        <div>@ViewData["phone"]</div>
                        <div>@ViewData["address"]</div>

                    </div>
                </div>

                @*<div style="background-color: #8686df;text-align:left;font-weight:bold;font-size:15px;padding-left:5px">Region : @ViewData["Region"]</div>*@
                <br>
                <div style="background-color: #8686df;text-align:left;font-weight:bold;font-size:15px;padding-left:5px">From :</div>
                <div>@Model.FinishTransfer.Fromname</div>
                <div style="background-color: #8686df;text-align:left;font-weight:bold;font-size:15px;padding-left:5px">T0 :</div>
                <div>@Model.FinishTransfer.Toname</div>
                @*<div style="background-color: #8686df;text-align:left;font-weight:bold;font-size:15px;padding-left:5px">TO :</div>
                    <div>@Model.distribution.ToStore</div>*@

                @*<div>@ViewData["customername"]</div>

                    <div>@ViewData["cusaddress"]</div>
                    <div>@ViewData["cusphone"]</div>

                    <div>@ViewData["cargo"]</div>*@




            </div>
            <div style=" padding: 10px; width: 60%; text-align: Center">
                <div style="color: blue; font-size: 18px; font-weight: bold">Transfer Finished Products</div>
                <br />
                <div><b>Inv#</b> <span style="border:1px solid; width: 100px; display: inline-block;">@Model.FinishTransfer.invid</span></div>
                <div><b>Date</b> <span style="border: 1px solid; width: 100px; display: inline-block; ">@Model.FinishTransfer.date.ToString("dd-MM-yyyy")</span></div>
                @*<div><b>From</b> <span style="border: 1px solid; width: 100px; display: inline-block; ">@Model.distribution.FromStore</span></div>
                    <div><b> To </b> <span style="border: 1px solid; width: 100px; display: inline-block; ">@Model.distribution.ToStore</span></div>*@
                <br>
                <div>@DateTime.Now.ToString("dd/MM/yyyy hh:mm:ss tt ")</div>
            </div>
        </div>




        <table class="table" style="border:1px solid">

            <thead>
                <tr class="text-center" style="background-color: #8686df">

                    <th style="width:80%"> Item Name</th>
                    <th>Packing</th>
                    <th>Quantity</th>
                

                </tr>
            </thead>
            <tbody>
                @if (!Model.FinishTransferDetailList.Any()) { }
                else
                {

                    foreach (var lst in Model.FinishTransferDetailList)
                    {


                        <tr>

                           
                            <td class="text-left">@lst.pname</td>
                            <td>@lst.size</td>
                            <td>@lst.qty</td>
                           


                        </tr>

                    }

                }
                <tr>
                    <td></td>
                    <td></td>
                
                    <td><b>@totalqty</b></td>


                </tr>


            </tbody>

        </table>

        @*<div style="text-align:right;font-weight:bold">Total : @total</div>*@

    </div>

    @*<script>
            window.onload = function () {
                window.print();
            };
        </script>*@


</body>
</html>








