@model IEnumerable<WebApplication1.QueryViewModel.Region>
@{
   
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    <div class="content">
        <div class="content-header">
            <div class="leftside-content-header">
                <ul class="breadcrumbs">
                    <li><a><i class="fa fa-cubes" aria-hidden="true"></i> Finished Goods Report</a></li>
                </ul>
            </div>
        </div>
        <h4 class="section-subtitle"><b>Finished Goods Report </b></h4>
        @using (Html.BeginForm("FinishedGoodsSearch", "Report", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="panel">
                <div class="panel-content">
                    <div class="form-group col-sm-3">
                        <label>Start Date</label>
                        <input type="date" name="s_date" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="form-control" required />
                    </div>
                    <div class="form-group col-sm-3">
                        <label>End Date </label>
                        <input type="date" name="e_date" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="form-control" required />
                    </div>
                    <div class="form-group col-sm-3">
                        <label>Select Region</label>
                        <select name="region" class="form-control">
                            <option value="0">Select Region</option>
                            @foreach (var list in Model)
                            {

                                <option value="@list.id">@list.name</option>
                            }
                        </select>
                    </div>
                    <br /><br />
                    <br />
                    <div class="col-sm-12">
                        <button type="submit" name="submit" class="btn btn-success" style="margin-top:25px"> &nbsp;&nbsp; Search &nbsp;&nbsp;</button>
                    </div>
                </div>
                <br /><br /><br />
            </div>

        }
    </div>
