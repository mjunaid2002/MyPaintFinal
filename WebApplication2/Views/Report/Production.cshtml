            <ul class="breadcrumbs">
                <li><a><i class="fa fa-print" aria-hidden="true"></i> Production</a></li>
            </ul>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <h4 class="section-subtitle"><b>Production  List</b></h4>
            @*<a href="@Url.Action("Purchases", "Procduction")" class="btn btn-wide btn-success pull-right" style="margin-top:-40px"><i class="fa fa-plus"></i>  Creat New </a>*@
            <div class="panel">
                <div class="panel-content">
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover table-striped table-success" id="searchable">
                            <thead class="bordered-darkorange">
                                <tr>
                                    @*<th>Inv Id</th>*@
                                    <th>Batch#</th>
                                    <th>Start Date</th>
                                    <th>End Date</th>
                                    <th>Invoice Cost</th>
                                    <th>Labour</th>
                                    <th>Other</th>
                                    <th>Total Cost</th>
                                    <th>Net Total</th>
                                    <th>Gorss Total</th>
                                    @*<th>Action</th>*@
                                </tr>
                            </thead>
                            <tbody class="abc">
                                @foreach (var item in Model.list)
                                {
                                    <tr>
                                        <td>@item.Rtotal</td>
                                        <td>@item.Date</td>
                                        <td>@item.Phone</td>
                                        <td>@item.NetAmount</td>
                                        <td>@item.CargoCharges</td>
                                        <td>@item.DiscountAmount</td>
                                        <td>@item.Total</td>
                                        <td>@item.NetAmount</td>
                                        <td>@item.GrandTotal</td>
                                        <!--<td>-->
                                            @*<a href="@Url.Action("PurReportView", new { ID= item.InvID } )" class="badge badge-xs x-warning b-rounded"><i class="fa fa-print"></i></a>*@
