</script>
<script>
    function taxx() {
        // alert("taxess");
        var am = document.getElementById("n_total").value;
        var pr = document.getElementById("sp").value;
        var qty = document.getElementById("qty").value;
        var dsc = document.getElementById("tax_per").value;
        var tax = (dsc / 100) * pr;
        var n = parseFloat(tax).toFixed(2) * parseFloat(qty).toFixed(2);
        document.getElementById("tax_amount").value = parseFloat(n).toFixed(2);
        var final = parseFloat(am) + parseFloat(n);
        document.getElementById("n_totalwt").value = parseFloat(final).toFixed(2);
    }
</script>


