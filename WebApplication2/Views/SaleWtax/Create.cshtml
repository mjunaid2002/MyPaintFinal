    function deleteRow(btn) {
        if (confirm("Are you sure want to delete the row?"))
            $(btn).closest("tr").remove();
        var table = document.getElementById("invoiceItem"), sumVal = 0;

        for (var i = 1; i < table.rows.length; i++) {
            sumVal = sumVal + parseInt(table.rows[i].cells[12].innerHTML);
        }
        //alert(sumVal);
        document.getElementById("net_final").value = sumVal;
        document.getElementById("g_total").value = sumVal;
        document.getElementById("r_total").value = sumVal;
        $('#d_amount').val('0');
        //$('#g_total').val('0');
        $('#cgo_charges').val('0');
        $('#total').val('0');
    }


    function tota() {

        var n_total = document.getElementById("n_total").value;
        var qty = document.getElementById("qty").value;
        if (qty == '' || qty == null) {
            document.getElementById("qty").value = "0";

        }

        var sp = document.getElementById("sp").value;
        if (sp == '' || sp == null) {
            document.getElementById("sp").value = "0";
        }

        var totl;
        totl = qty * sp;
        if (n_total == '' || n_total == null) {
            document.getElementById("n_total").value = "0";
        }
        document.getElementById("n_total").value = totl;
        if (n_total == '' || n_total == null) {
            document.getElementById("n_total").value = "0";
        }
    }
    function gt() {
        var cgo_charges = document.getElementById("cgo_charges").value;
        var total = document.getElementById("total").value;
        var nt = document.getElementById("net_final").value;
        var ntwt = document.getElementById("net_finalwot").value;
        var t_f = parseFloat(cgo_charges) + parseFloat(nt);
        var t_fwt = parseFloat(cgo_charges) + parseFloat(ntwt);
        document.getElementById("total").value = t_f;
        var da = document.getElementById("d_amount").value;
        document.getElementById("d_amount").value = "0";
        document.getElementById("g_total").value = t_f;
        document.getElementById("g_totalwot").value = t_fwt;
        document.getElementById("r_total").value = t_f;
    }
    function gt1() {
        var da = document.getElementById("d_amount").value;
        var total = document.getElementById("total").value;
        var ntwt = document.getElementById("net_finalwot").value;
        var totl = parseFloat(total) - parseFloat(da);
        var t_fwt = parseFloat(ntwt) - parseFloat(da);
        document.getElementById("g_total").value = totl;
        document.getElementById("g_totalwot").value = t_fwt;
        document.getElementById("r_total").value = totl;
    }

</script>
<script type="text/javascript">
