@{
    ViewBag.Title = "Index";
    int sr = 0;

}
<script>
    window.onload = function () {
        var chart = new CanvasJS.Chart("chartContainer", {
            animationEnabled: true,
            theme: "light2", // "light1", "light2", "dark1", "dark2"
            title: {
                text: "Monthly  Sale"
            },
            axisY: {
                title: "Total Amount (Rs)"
            },
            data: [{
                type: "column",
                showInLegend: true,
                legendMarkerColor: "grey",
                legendText: "Sale Graph",
                dataPoints: [

                    @foreach (var item in Model.sale_list)
                    {
                        @:{ y: @(item.total), label: '@(Convert.ToDateTime(item.Date).ToString("dd-MM-yyyy"))' },
                   }

                ]
            }]
        });
        var chartsale = new CanvasJS.Chart("chartContainersale", {
            animationEnabled: true,
            theme: "light2", // "light1", "light2", "dark1", "dark2"
            title: {
                text: "Monthly  Sale"
            },
            axisY: {
                title: "Total Amount (Rs)"
            },
            data: [{
                type: "column",
                showInLegend: true,
                legendMarkerColor: "grey",
                legendText: "Sale Graph",
                dataPoints: [

                    @foreach (var item in Model.sale_list)
                    {
                        @:{ y: @(item.total), label: '@(Convert.ToDateTime(item.Date).ToString("dd-MM-yyyy"))' },
                   }

                ]
            }]
        });
        var chart1 = new CanvasJS.Chart("chartContainer1", {
            animationEnabled: true,
            theme: "light2", // "light1", "light2", "dark1", "dark2"
            title: {
                text: "Monthly Purchase"
            },
            axisY: {
                title: "Total Amount (Rs)"
            },
            data: [{
                type: "column",
                showInLegend: true,
                legendMarkerColor: "grey",
                legendText: "Purchase Graph",
                dataPoints: [
                     @foreach (var item1 in Model.pur_dash)
                    {
                        @:{ y: @(item1.total), label: '@(Convert.ToDateTime(item1.Date).ToString("dd-MM-yyyy"))' },
                   }

                ]
            }]
        });
        chart.render();
        chart1.render();
        chartsale.render();
        var chart2 = new CanvasJS.Chart("chartContainer2", {
            animationEnabled: true,
            theme: "light2",
            title: {
                text: "Sale Summmary"
            },


            data: [{
                type: "line",
                showInLegend: true,
                //name: "Total Visit",
                markerType: "square",
                //xValueFormatString: "DD MMM, YYYY",
                color: "#F08080",
                dataPoints: [
                    @foreach (var item in Model.sale_list)
                    {
                        @:{ x: new Date('@(Convert.ToDateTime(item.Date).Year)', '@(Convert.ToDateTime(item.Date).Month)', '@(Convert.ToDateTime(item.Date).Day)'), y: @(item.total) },
                   }

                ]
            },
            {
                type: "line",
                showInLegend: true,
                name: "Unique Visit",
                lineDashType: "dash",
                dataPoints: [

                ]
            }]
        });
        chart2.render();
        function toogleDataSeries(e) {
            if (typeof (e.dataSeries.visible) === "undefined" || e.dataSeries.visible) {
                e.dataSeries.visible = false;
            } else {
                e.dataSeries.visible = true;
            }
            chart.render();
        chart1.render();
        chartsale.render();
        }


    }

</script>
