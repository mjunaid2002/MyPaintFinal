<!doctype html>
<html lang="en" class="fixed">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>POS</title>
    <link rel="apple-touch-icon" sizes="120x120" href="~/Content/favicon/apple-icon-120x120.png">
    <link rel="icon" type="image/png" sizes="192x192" href="~/Content/favicon/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/Content/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/favicon/favicon-16x16.png">
    <script src="~/Content/vendor/pace/pace.min.js"></script>
    <link href="~/Content/vendor/pace/pace-theme-minimal.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/vendor/bootstrap/css/bootstrap.css">
    <link rel="stylesheet" href="~/Content/vendor/font-awesome/css/font-awesome.css">
    <link rel="stylesheet" href="~/Content/vendor/animate.css/animate.css">
    <link rel="stylesheet" href="~/Content/vendor/toastr/toastr.min.css">
    <link rel="stylesheet" href="~/Content/vendor/magnific-popup/magnific-popup.css">
    <link rel="stylesheet" href="~/Content/stylesheets/css/style.css">
    <link rel="stylesheet" href="~/Content/vendor/data-table/media/css/dataTables.bootstrap.min.css">
    <link href="~/Content/Datatable/dataTables.bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/choosen/chosen.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <link rel="stylesheet" href="~/Content/Touch/css/style.css">
    <script src="~/Content/Touch/js/js.js"></script>
    <script src="~/Content/Touch/js/js.js1"></script>
    <style>
        html {
            font-size: 62.5%;
            box-sizing: border-box;
        }

        *,
        *::before,
        *::after {
            margin: 0;
            padding: 0;
            box-sizing: inherit;
        }

        .calculator {
            border: 1px solid #ccc;
            border-radius: 5px;
            margin-left: -100px;
            width: 400px;
        }

        .calculator-screen {
            width: 100%;
            height: 80px;
            border: none;
            background-color: #252525;
            color: #fff;
            font-size: 4rem;
        }

        button {
        }

        .equal-sign {
            height: 98%;
            grid-area: 2 / 4 / 6 / 5;
        }

        .calculator-keys {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            grid-gap: 20px;
            padding: 20px;
        }
    </style>
    <style>
        .abc {
            display: table-footer-group;
            vertical-align: middle;
            border-color: inherit;
        }

            .abc tr td {
                font-size: 11px;
            }

        .form-control {
            height: 30px;
            padding: 5px 10px;
            font-size: 12px;
            line-height: 1.5;
            border-radius: 3px;
        }

        .chosen-container {
            height: 27px;
            font-size: 12px;
            line-height: 1.5;
            border-radius: 3px;
        }

        modal-backdrop {
            display: none;
        }

        .modal-backdrop.in {
            filter: alpha(opacity=50);
            opacity: .5;
        }
    </style>
</head>
<body onload="date();window.print();">
    <div class="wrap">
        <div class="page-header" style="margin-top:-0px">
            <div class="leftside-header">
                <div class="logo">
                    <a asp-controller="Home" asp-action="Index" class="on-click">
                        <center>
                            <h2 style="color:white;margin-top:10px;">
                                HCL
                            </h2>
                        </center>
                    </a>
                </div>
                <div id="menu-toggle" class="visible-xs toggle-left-sidebar" data-toggle-class="left-sidebar-open" data-target="html">
                    <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                </div>
            </div>
            <div class="rightside-header">
                <div class="header-middle"></div>
                <div class="header-section" id="search-headerbox">
                    <input type="text" name="search" id="search" placeholder="Search...">
                    <i class="fa fa-search search" id="search-icon" aria-hidden="true"></i>
                    <div class="header-separator"></div>
                </div>
                <div class="header-section">
                    <a asp-controller="Home" asp-action="Logout" data-toggle="tooltip" data-placement="left" title="Logout"><i class="fa fa-sign-out log-out" aria-hidden="true"></i></a>
                </div>
            </div>
        </div>
        <div class="page-body">
            @RenderBody()

            <a href="#" class="scroll-to-top"><i class="fa fa-angle-double-up"></i></a>
        </div>
    </div>
    <script src="~/Content/js/ckeditor/ckeditor.js"></script>
    <script src="~/Content/choosen/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="~/Content/choosen/chosen.jquery.js" type="text/javascript"></script>
    <script src="~/Content/choosen/prism.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/Content/choosen/init.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/Content/vendor/jquery/jquery-1.12.3.min.js"></script>
    <script src="~/Content/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/vendor/nano-scroller/nano-scroller.js"></script>
    <script src="~/Content/javascripts/template-script.min.js"></script>
    <script src="~/Content/javascripts/template-init.min.js"></script>
    <script src="~/Content/vendor/data-table/media/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/vendor/data-table/media/js/dataTables.bootstrap.min.js"></script>
    <script src="~/Content/javascripts/examples/tables/data-tables.js"></script>
    <script src="~/Content/javascripts/examples/ui-elements/buttons.js"></script>
    <script src="~/Content/Datatable/jquery.min.js"></script>
    <script src="~/Content/Datatable/jquery.dataTables.min.js"></script>
    <script src="~/Content/Datatable/dataTables.tableTools.min.js"></script>
    <script src="~/Content/Datatable/dataTables.bootstrap.min.js"></script>
    <script src="~/Content/Datatable/datatables-init.js"></script>
    <script src="~/Content/javascripts/examples/ui-elements/alerts.js"></script>
    <script>
        InitiateSimpleDataTable.init();
        InitiateEditableDataTable.init();
        InitiateExpandableDataTable.init();
        InitiateSearchableDataTable.init();
    </script>

</body>
</html>
