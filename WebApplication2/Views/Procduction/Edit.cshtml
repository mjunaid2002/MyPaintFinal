            htmlRows += '<td><input type="text"  name="qty[]"  value="' + qty + '" onkeyup="qty12333();"  class="form-control" autocomplete="off"></td>';
            htmlRows += '<td><input type="text"  name="n_total[]"  value="' + n_total + '"   readonly class="form-control" autocomplete="off"></td>';
