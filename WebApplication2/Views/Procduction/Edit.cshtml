    function deleteRow(btn) {
        if (confirm("Are you sure want to delete the row?"))
            $(btn).closest("tr").remove();

        var pur_leng = document.getElementById("pur_leng").value;
        var net_final = document.getElementById("p_cost").value;
        var table = document.getElementById("invoiceItem"), sumVal = 0;
        for (var i = 1; i < table.rows.length; i++) {
            sumVal = sumVal + parseInt(table.rows[i].cells[9].innerHTML);
        }
        var net_final = document.getElementById("p_cost").value;
        var table = document.getElementById("invoiceItem"), sumVal = 0;
        document.getElementById("total_cost").value = parseFloat(sumVal) + parseFloat(net_final);
        var table = document.getElementById("invoiceItem"), sumVal1 = 0;
        for (var i = 1; i < table.rows.length; i++) {
            sumVal1 = sumVal1 + parseInt(table.rows[i].cells[9].innerHTML);
        }
        document.getElementById("pur_leng").value = parseFloat(sumVal1) + parseFloat(pur_leng);
        qty123();
        qty12333();
        gt();
        //  $('#d_amount').val('0');
        //$('#g_total').val('0');
        // $('#cgo_charges').val('0');
        // $('#total').val('0');
    }


    function tota() {
        //  alert("text");
        var p_box = document.getElementById("length").value;
        var ctn = document.getElementById("ctn").value;
        var n_total = document.getElementById("n_total").value;
        qty_f = ctn;

        var sp = document.getElementById("sp").value;
        var totl;
        totl = qty_f * sp;
        if (n_total == '' || n_total == null) {
            document.getElementById("n_total").value = "0";
        }
        document.getElementById("n_total").value = totl;
        if (n_total == '' || n_total == null) {
            document.getElementById("n_total").value = "0";
        }
    }
    function gt() {
        var cgo_charges = document.getElementById("cgo_charges").value;
        var fin_net_total = document.getElementById("fin_net_total").value;
        var table = document.getElementById("invoiceItem"), sumVal = 0;
        for (var i = 1; i < table.rows.length; i++) {
            sumVal = sumVal + parseFloat(table.rows[i].cells[10].innerHTML);
        }
        // document.getElementById("total_cost").value = parseFloat(sumVal);

        var dis_amount = document.getElementById("dis_amount").value;
        // alert(sumVal);
        var t_f = parseFloat(cgo_charges) + parseFloat(dis_amount) + parseFloat(sumVal);
        document.getElementById("total_cost").value = t_f;
        document.getElementById("gross").value = fin_net_total - t_f;

        //qty12333();
        // document.getElementById("gross").value = (-1)*(t_f);
    }
    function qty123() {
        // alert("dsa");
        var grandTotal = 0;
        var grandTotal1 = 0.00;

        $("input[name='qty_ne[]']").each(function (index) {
            var price = $("input[name='sp_ne[]']").eq(index).val();
            var qty_ne = $("input[name='qty_ne[]']").eq(index).val();
            var length = $("input[name='length[]']").eq(index).val();
            var ctn = qty_ne * length;
            var output = parseFloat(price) * parseFloat(qty_ne);
            if (!isNaN(output)) {

                $("input[name='tot_leng[]']").eq(index).val(ctn.toFixed(2));
                $("input[name='tot_ne[]']").eq(index).val(output.toFixed(2));
                grandTotal = parseFloat(grandTotal) + parseFloat(output);
                grandTotal1 = parseFloat(grandTotal1) + parseFloat(ctn);
                $('#fin_net_total').val(grandTotal);
                $('#pro_leng').val(grandTotal1.toFixed(2));
                var t_cost = document.getElementById('total_cost').value;
                var pur_leng = document.getElementById('pur_leng').value;
                $('#yelid').val(((grandTotal1 / pur_leng) * 100).toFixed(2));
                $('#gross').val(parseFloat(grandTotal) - parseFloat(t_cost));
            }

        });
    }
    function qty12333() {
        // alert("dsa");
        var grandTotal = 0;
        var grandTotal1 = 0;

        $("input[name='qty[]']").each(function (index) {
            var qty = $("input[name='qty[]']").eq(index).val();
            var sp = $("input[name='sp[]']").eq(index).val();
            var length = $("input[name='length[]']").eq(index).val();
            var output = ((parseFloat(qty) * parseFloat(length)) * parseFloat(sp));
            var t_len = ((parseFloat(qty) * parseFloat(length)));
            //  alert(output);
            if (!isNaN(output)) {
                $("input[name='n_total[]']").eq(index).val(output.toFixed(2));
                grandTotal = parseFloat(grandTotal) + parseFloat(output);
                grandTotal1 = parseFloat(grandTotal1) + parseFloat(t_len);
                document.getElementById("total_cost").value = grandTotal.toFixed(2);
                document.getElementById("pur_leng").value = grandTotal1.toFixed(2);
                var fin_net_total = document.getElementById('fin_net_total').value;
                $('#gross').val(parseFloat(fin_net_total) - parseFloat(grandTotal));
                //document.getElementById("g_total").value = grandTotal.toFixed(2);
            }

        });
    }


</script>
