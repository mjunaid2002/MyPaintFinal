@model IEnumerable<WebApplication1.QueryViewModel.LabMasterQuery>
@{
    ViewBag.Title = "Index";
}
<script>
    $('#searchable').DataTable({
        buttons: [
            'copy', 'excel', 'pdf'
        ]
    });
</script>
<div class="content">
    <div class="content-header">
        <div class="leftside-content-header">
            <ul class="breadcrumbs">
                <li><a><i class="fa fa-cubes" aria-hidden="true"></i> Lab </a></li>
            </ul>
        </div>
    </div>
    <div class="row">
        <h4 class="section-subtitle"><b> Lab Entries List</b></h4>



        @using (Html.BeginForm("Index", "LabEntries", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form-group col-sm-3">
                <label>Start Date</label>
                <input type="date" name="s_date" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="form-control" required />
            </div>
            <div class="form-group col-sm-3">
                <label>End Date </label>
                <input type="date" name="e_date" value="@DateTime.Now.ToString("yyyy-MM-dd")" class="form-control" required />
            </div>
            <div class="col-sm-3">
                <button type="submit" name="submit" class="btn btn-success" style="margin-top:25px"> &nbsp;&nbsp; Search &nbsp;&nbsp;</button>
            </div>
        }

        @if (Session["SubLabAdd"].ToString() != "0")
        {
            <a href="@Url.Action("Create", "LabEntries")" class="btn btn-wide btn-success pull-right" style="margin-top:-40px"><i class="fa fa-plus"></i>  Creat New</a>
        }
    </div>
    <div class="row">
        <div class="col-sm-12">

            <div class="panel">
                <div class="panel-content">
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover table-striped table-success" id="searchable">
                            <thead class="bordered-darkorange">
                                <tr>
                                    <th>OrderId</th>
                                    <th>Date</th>
                                    <th>Batch No</th>
                                    <th>Color Name</th>
                                    <th>Product Name</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody class="abc">
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item.OrderId</td>
                                        <td>@item.Date.ToString("dd-MM-yyyy")</td>
                                        <td>@item.BatchNo</td>
                                        <td>@item.ColorName</td>
                                        <td>@item.ProductName</td>
                                        <td>
                                            @if (Session["SubLabUpdate"].ToString() != "0")
                                            {

                                                <a href="@Url.Action("Edit", new { ID = item.OrderId })" new { onclick="return confirm('Are you sure you want to Edit this record?');" class="label label-warning shiny input-xs"><i class="fa fa-pencil"></i></a>
                                                @*if (item.ready != true)
                                                    {
                                                    }
                                                    else
                                                    {
                                                        <a href="@Url.Action("statusrequest", new { ID = item.OrderId})" new { onclick="return confirm('Are you sure you want to Send Edit Request for this Lab ?');" class="label label-default shiny input-xs"><i class="fa fa-key"></i></a>
                                                    }*@
                                            }
                                            else
                                            {
                                                if (item.ready != true)
                                                {
                                                    <a href="@Url.Action("Edit", new { ID = item.OrderId })" new { onclick="return confirm('Are you sure you want to Edit this record?');" class="label label-warning shiny input-xs"><i class="fa fa-pencil"></i></a>

                                                }
                                                else
                                                {
                                                    if (item.req_status == "Request")
                                                    {
                                                        <a href="@Url.Action("statusrequest", new { ID = item.OrderId })" new { onclick="return confirm('Are you sure you want to Send Edit Request for this Lab?');" class="label label-default shiny input-xs"><i class="fa fa-key"></i></a>
                                                    }
                                                    
                                                    else if (item.req_status == "Pending")
                                                    {
                                                        <a new { onclick="return confirm('your already Send Edit Request for this Sale Invoice ?');" class="label label-warning shiny input-xs"><i class="fa fa-key"></i></a>

                                                    }
                                                    else
                                                    {
                                                        <a href="@Url.Action("Edit", new { ID = item.OrderId })" new { onclick="return confirm('Are you sure you want to Edit this record?');" class="label label-warning shiny input-xs"><i class="fa fa-pencil"></i></a>

                                                    }
                                                }
                                            }
                                            @if (Session["SubLabView"].ToString() != "0")
                                            {
                                                @*<a href="@Url.Action("InvoiceReport", new { ID= item.OrderId } )" class="label label-success shiny input-xs"><i class="fa fa-print"></i></a>*@
                                                <a href="@Url.Action("InvoiceReport", new { ID= item.OrderId,type="pdf" } )" class="label label-success shiny input-xs"><i class="fa fa-print"></i></a>
                                                <a href="@Url.Action("InvoiceReport", new { ID= item.OrderId,type="show" }  )" class="label label-primary shiny input-xs" target="_blank"><i class="fa fa-print"></i></a>
                                                <a href="@Url.Action("InvoiceReport", new { ID= item.OrderId,type="show1" }  )" class="label label-default shiny input-xs" target="_blank"><i class="fa fa-print"></i></a>
                                            }

                                        </td>
                                    </tr>
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th><input type="text" name="search_browser" placeholder="Search Here" class="form-control input-sm"></th>
                                    <th><input type="text" name="search_browser" placeholder="Search Here" class="form-control input-sm"></th>
                                    <th><input type="text" name="search_browser" placeholder="Search Here" class="form-control input-sm"></th>
                                    <th><input type="text" name="search_browser" placeholder="Search Here" class="form-control input-sm"></th>
                                    <th><input type="text" name="search_browser" placeholder="Search Here" class="form-control input-sm"></th>
                                    <th></th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
