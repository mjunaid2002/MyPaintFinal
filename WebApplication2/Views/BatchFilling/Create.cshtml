        var total_batch_packing_quarter =  document.getElementById("total_batch_packing_quarter").value;
        var total_batch_packing_gallon =  document.getElementById("total_batch_packing_gallon").value;
        var total_batch_packing_drum = document.getElementById("total_batch_packing_drum").value;

        var sample_dubbi = document.getElementById("sample_dubbi").value;
        var sample_quarter = document.getElementById("sample_quarter").value;
        var sample_gallon = document.getElementById("sample_gallon").value;
        var sample_drum = document.getElementById("sample_drum").value;

        document.getElementById("per_unit_dubbi").value = parseFloat(total_batch_packing_dabbi) / parseFloat(Dubi_nos);
        document.getElementById("per_unit_quarter").value = parseFloat(total_batch_packing_quarter) / parseFloat(Quarter_nos);
        document.getElementById("per_unit_gallon").value = parseFloat(total_batch_packing_gallon) / parseFloat(Gallon_nos);
        document.getElementById("per_unit_drum").value = parseFloat(total_batch_packing_drum) / parseFloat(Drum_nos);


        document.getElementById("total_filling_weight").value = parseFloat(sample_dubbi) + parseFloat(sample_quarter) + parseFloat(sample_gallon) + parseFloat(sample_drum) +parseFloat(Dubi_we) + parseFloat(Gallon_we) + parseFloat(Drum_we) + parseFloat(Quarter_we);


        document.getElementById("total_batch_cost").value = parseFloat(total_batch_cost_dabbi) + parseFloat(total_batch_cost_quarter) + parseFloat(total_batch_cost_gallon) + parseFloat(total_batch_cost_drum);


        document.getElementById("total_final_cost").value = parseFloat(total_batch_packing_dabbi) + parseFloat(total_batch_packing_quarter) + parseFloat(total_batch_packing_gallon) + parseFloat(total_batch_packing_drum);

        var total_filling_weight = document.getElementById("total_filling_weight").value;
        var total_f_qty = document.getElementById("total_f_qty").value;
        document.getElementById("yeild").value = parseFloat(total_filling_weight) / parseFloat(total_f_qty) *100;
    }

