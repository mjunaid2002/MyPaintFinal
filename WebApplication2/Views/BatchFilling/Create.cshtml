        var total_packing_gallon = document.getElementById("total_packing_gallon").value;
        var total_packing_drum = document.getElementById("total_packing_drum").value;


        document.getElementById("total_batch_packing_dabbi").value = parseFloat(total_batch_cost_dabbi) + parseFloat(total_packing_dubbi);
        document.getElementById("total_batch_packing_quarter").value = parseFloat(total_batch_cost_quarter) + parseFloat(total_packing_quarter);
        document.getElementById("total_batch_packing_gallon").value = parseFloat(total_batch_cost_gallon) + parseFloat(total_packing_gallon);
        document.getElementById("total_batch_packing_drum").value = parseFloat(total_batch_cost_drum) + parseFloat(total_packing_drum);


