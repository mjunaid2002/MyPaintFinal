        var Quarter_we = document.getElementById("Quarter_we").value;
        var Gallon_we = document.getElementById("Gallon_we").value;
        var Drum_we = document.getElementById("Drum_we").value;



        document.getElementById("Dubi_diff").value = Dubi_tot - Dubi_we;
        document.getElementById("Quarter_diff").value = Quarter_tot - Quarter_we;
        document.getElementById("Gallon_diff").value = Gallon_tot - Gallon_we;
        document.getElementById("Drum_diff").value = Drum_tot - Drum_we;



        var total_bat_cost = document.getElementById("total_cost").value;

        document.getElementById("total_batch_cost_dabbi").value = Dubi_we * total_bat_cost;
        document.getElementById("total_batch_cost_quarter").value = Quarter_we * total_bat_cost;
        document.getElementById("total_batch_cost_gallon").value = Gallon_we * total_bat_cost;
        document.getElementById("total_batch_cost_drum").value = Drum_we * total_bat_cost;

        var total_batch_cost_dabbi = document.getElementById("total_batch_cost_dabbi").value;
        var total_batch_cost_quarter = document.getElementById("total_batch_cost_quarter").value;
        var total_batch_cost_gallon = document.getElementById("total_batch_cost_gallon").value;
        var total_batch_cost_drum = document.getElementById("total_batch_cost_drum").value;



