                                                            @Html.TextBoxFor(m => m.batchFillingMasterQUery.SampleGallonWeight, new { @class = "form-control input-xs", style = "background-color:#00CCFE;color:black", value = "0", id = "sample_gallon", @onKeyUp = "tota3();" })
                                                        </div>
                                                        <div class="form-group" style="margin-left:-10px;width:70px !important">
                                                            @Html.TextBoxFor(m => m.batchFillingMasterQUery.SampleDrumWeight, new { @class = "form-control input-xs", style = "background-color:#00CCFE;color:black", value = "0", id = "sample_drum", @onKeyUp = "tota3();" })
                                                        </div>
                                                    </center>
                                                </div>
                                            </div>
                                            <div class="row">
                                                </br>
                                                <div class="form-group col-sm-3">
                                                    <label>Total Filling Weight</label>
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.totalfillweight, new { @class = "form-control input-xs", id = "total_filling_weight" })
                                                </div>
                                                <div class="form-group col-sm-3">
                                                    <label>Yield</label>
                                                    <input type="hidden" id="total_f_qty" class="form-control" />
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.yeild, new { @class = "form-control input-xs", id = "yeild" })
                                                </div>
                                                <div class="form-group col-sm-3 @isHidden">
                                                    <label>Batches Per KG Cost</label>
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.batchespercost, new { @class = "form-control input-xs", id = "total_cost" })
                                                </div>
                                                <div class="form-group col-sm-3">
                                                    <label>Total Paking Cost</label>
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.packingcost, new { @class = "form-control input-xs", id = "total_packing_cost" })
                                                </div>
                                                <div class="form-group col-sm-3 @isHidden">
                                                    <label>Total Batch Cost</label>
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.batchcost, new { @class = "form-control input-xs", id = "total_batch_cost" })
                                                </div>
                                                <div class="form-group col-sm-3 @isHidden">
                                                    <label>Total Cost</label>
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.totalcost, new { @class = "form-control input-xs", id = "total_final_cost" })
                                                </div>
                                                <div class="form-group col-sm-3">
                                                    <label>Supervised By</label>
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.supervisedby, new { @class = "form-control input-xs" })
                                                </div>
                                                <div class="form-group col-sm-3">
                                                    <label>Filling 1 By</label>
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.filledby, new { @class = "form-control input-xs" })
                                                </div>
                                                <div class="form-group col-sm-3">
                                                    <label>Filling 2 By</label>
                                                    @Html.TextBoxFor(m => m.batchFillingMasterQUery.filledby1, new { @class = "form-control input-xs" })
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-12">
                                        @if (Session["SubBatchFillingAdd"].ToString() != "0")
                                        {
                                            <button type="submit" class="btn btn-primary col-sm-4 col-sm-offset-4" style="margin-top:10px"><i class="fa fa-save"></i> &nbsp; Save Data</button>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="lg-modal" aria-labelledby="modal-large-label" style="margin-top:-180px">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header state modal-primary" style="background-color: #3498DB !important">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="modal-large-label">Product Details</h4>
            </div>
            <div class="modal-body">
